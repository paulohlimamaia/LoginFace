{
    "expo": {
        "name": "React Native Simple Login",
        "slug": "react-native-simple-login",
        "privacy": "public",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "package": "com.paulohenrique.login",
            "versionCode": 1
        },
        "ios": {
            "supportsTablet": true
        },
        "description": "Simple login created with React Native."
    }
}